<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="128dp"
    android:height="128dp"
    android:viewportWidth="128"
    android:viewportHeight="128">
    <group
        android:name="pauseicongroup"
        android:pivotX="64"
        android:pivotY="64"
        android:scaleX="1"
        android:scaleY="1">

        <group android:name="pauseicongrouptop">
            <path
                android:name="pauseicontoppathname"
                android:pathData="@string/pause_icon_upper_path_data"
                android:fillColor="@android:color/white"
                android:strokeLineCap="butt"
                android:strokeLineJoin="miter"
                android:strokeMiterLimit="10"/>
        </group>
        <group android:name="pauseicongroupbottom">
            <path
                android:name="pauseiconbottompathname"
                android:pathData="@string/pause_icon_bottom_path_data"
                android:fillColor="@android:color/white"
                android:strokeLineCap="butt"
                android:strokeLineJoin="miter"
                android:strokeMiterLimit="10"/>
        </group>

    </group>
</vector>